{
  "name": "focused-explorer",
  "displayName": "Focused Explorer",
  "description": "Extension to keep only the active file ancestors folders expanded. The other folders will be collapsed.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasPereira/focused-explorer"
  },
  "engines": {
    "vscode": "^1.99.3"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["*"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "focused-explorer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.99.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  }
}
